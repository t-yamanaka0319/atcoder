   "source": [
    "# 入力を受ける\n",
    "S_x,S_y,G_x,G_y = map(int, input().split())\n",
    "\n",
    "# G_yを反転させる\n",
    "G_y = G_y * -1\n",
    "\n",
    "# S,Gの座標から1次関数を作る\n",
    "a = (G_y - S_y)/(G_x -S_x)\n",
    "b = S_y -a*S_x\n",
    "\n",
    "# x軸上の座標を求める\n",
    "x = (-b)/a\n",
    "\n",
    "# 出力する\n",
    "print(x)"
   ]
