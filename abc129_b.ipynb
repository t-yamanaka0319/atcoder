   "source": [
    "N = int(input())\n",
    "W = list(map(int, input().split()))\n",
    "\n",
    "g = sum(W)\n",
    "\n",
    "ans = g\n",
    "for i in range(1,N):\n",
    "    sum_i = sum(W[0:i])\n",
    "    div = abs((sum_i)-(g-sum_i))\n",
    "    if ans > div:\n",
    "        ans = div\n",
    "#     print('sum_i:' + str(sum_i))\n",
    "print(ans)"
