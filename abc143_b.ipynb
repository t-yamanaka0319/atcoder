    "import itertools as ite\n",
    "\n",
    "# 入力を受ける\n",
    "N = int(input())\n",
    "d = list(map(int,input().split()))\n",
    "\n",
    "# dから2個選び、総和をとる\n",
    "rec = 0\n",
    "for tako in ite.combinations(d, 2):\n",
    "    rec += tako[0]*tako[1]\n",
    "print(rec)"
   ]
